{
  "name": "taskmarks",
  "displayName": "Taskmarks",
  "description": "Little helper for teams to navigate through files. Persists bookmarks for different tasks.",
  "version": "0.8.7",
  "publisher": "NorbertKlein",
  "contributors": [
    "Klein Norbert",
    "Brent Whitehead"
  ],
  "engines": {
    "vscode": "^1.65.0"
  },
  "license": "(MIT OR Apache-2.0)",
  "icon": "images/taskmarks-icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Bookmark, Tasks, Remember, Places, Teamwork, Collaboration"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "taskmark.selectMarkFromList",
        "title": "Taskmarks: Select Bookmark from List"
      },
      {
        "command": "taskmark.selectTask",
        "title": "Taskmarks: Select Active Task"
      },
      {
        "command": "taskmark.createTask",
        "title": "Taskmarks: Create new Task"
      },
      {
        "command": "taskmark.deleteTask",
        "title": "Taskmarks: Delete Task"
      },
      {
        "command": "taskmark.toggleMark",
        "title": "Taskmarks: Toggle Bookmark at Current Position"
      },
      {
        "command": "taskmark.nextMark",
        "title": "Taskmarks: Find next Bookmark"
      },
      {
        "command": "taskmark.previousMark",
        "title": "Taskmarks: Find previous Bookmark"
      }
    ],
    "keybindings": [
      {
        "command": "taskmark.previousMark",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      },
      {
        "command": "taskmark.selectTask",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      },
      {
        "command": "taskmark.toggleMark",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      },
      {
        "command": "taskmark.nextMark",
        "key": "ctrl+shift+z",
        "mac": "cmd+shift+z",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "prettify": "prettier -l --write \"**/*.ts\"",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pwsh scripts\\fastDelete.ps1 out",
    "pretestXX": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest --runInBand --detectOpenHandles",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "checkForUpgrade": "ncu",
    "upgrade": "ncu -u && npm install",
    "build package": "vsce package",
    "publish": "vsce publish",
    "publish pre-release": "vsce publish --pre-release"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vscode/test-electron": "^2.1.3",
    "esbuild": "^0.14.28",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "glob": "^7.2.0",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.4",
    "prettier": "2.6.1",
    "prettier-eslint-cli": "5.0.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norbertK/taskmarks"
  },
  "homepage": "https://github.com/norbertK/taskmarks/blob/master/README.md",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
