{
  "name": "taskmarks",
  "displayName": "Taskmarks",
  "description": "Little helper for teams to navigate through files. Persists bookmarks for different tasks.",
  "version": "0.8.13",
  "publisher": "NorbertKlein",
  "contributors": [
    "Klein Norbert",
    "Brent Whitehead"
  ],
  "engines": {
    "vscode": "^1.66.0"
  },
  "license": "(MIT OR Apache-2.0)",
  "icon": "images/taskmarks-icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Bookmark, Tasks, Remember, Places, Teamwork, Collaboration"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "taskmarks.selectMarkFromList",
        "title": "Taskmarks: Select Bookmark from List"
      },
      {
        "command": "taskmarks.selectTask",
        "title": "Taskmarks: Select Active Task"
      },
      {
        "command": "taskmarks.createTask",
        "title": "Taskmarks: Create new Task"
      },
      {
        "command": "taskmarks.deleteTask",
        "title": "Taskmarks: Delete Task"
      },
      {
        "command": "taskmarks.toggleMark",
        "title": "Taskmarks: Toggle Bookmark at Current Position"
      },
      {
        "command": "taskmarks.nextMark",
        "title": "Taskmarks: Find next Bookmark"
      },
      {
        "command": "taskmarks.previousMark",
        "title": "Taskmarks: Find previous Bookmark"
      }
    ],
    "keybindings": [
      {
        "command": "taskmarks.previousMark",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      },
      {
        "command": "taskmarks.selectTask",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      },
      {
        "command": "taskmarks.toggleMark",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      },
      {
        "command": "taskmarks.nextMark",
        "key": "ctrl+shift+z",
        "mac": "cmd+shift+z",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "prettify": "prettier -l --write \"**/*.ts\"",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "pretestJ": "pwsh scripts\\fastDelete.ps1 out",
    "lint": "eslint src --ext ts",
    "test": "jest --runInBand --detectOpenHandles",
    "test-compile": "tsc -p ./",
    "checkForUpgrade": "ncu",
    "upgrade": "ncu -u && npm install",
    "inst": "npm install",
    "build package": "vsce package",
    "publish": "vsce publish",
    "publish pre-release": "vsce publish --pre-release"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "17.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.12.0",
    "glob": "^7.2.0",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.7",
    "prettier": "2.6.2",
    "prettier-eslint-cli": "5.0.1",
    "ts-loader": "^9.2.8",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norbertK/taskmarks"
  },
  "homepage": "https://github.com/norbertK/taskmarks/blob/master/README.md",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "shared"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
